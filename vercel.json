{
  "version": 2,
  "name": "mugic",
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/static/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9"
    }
  },
  "functions": {
    "api/index.py": {
      "maxDuration": 60,
      "memory": 3008
    }
  },
  "regions": ["iad1"],
  "github": {
    "silent": false
  }
}
